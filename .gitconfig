[user]
	name = Ellis Kenyo
	email = me@elken.dev
[push]
	default = current
[github]
	user = elken
[core]
	excludefile = /home/elken/.gitignore
	filemode = false
[alias]
      pu = !"git fetch origin -v; git fetch upstream -v; git merge upstream/master"
[recieve]
        denyCurrentBranch = ignore
[gpg]
	program = /opt/homebrew/bin/gpg
[commit]
	gpgSign = true
[tag]
	forceSignAnnotated = true
[init]
	defaultBranch = "master"
[diff "org"]
  xfuncname = "^(\\*+ +.*)$"
